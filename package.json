{
  "name": "store-cataloguer-backend",
  "version": "1.0.0",
  "description": "Production-ready backend for admin dashboard",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "db:migrate": "tsx src/db/migrate.ts migrate",
    "db:migrate:rollback": "tsx src/db/migrate.ts rollback",
    "db:migrate:status": "tsx src/db/migrate.ts status",
    "db:generate": "tsx src/db/migration-generator.ts",
    "db:seed": "tsx src/db/seed.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "express",
    "typescript",
    "mysql",
    "jwt",
    "rbac"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@types/html-pdf": "^3.0.3",
    "argon2": "^0.31.2",
    "axios": "^1.12.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "html-pdf": "^3.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "mysql2": "^3.6.3",
    "nodemailer": "^6.9.4",
    "pdfmake": "^0.2.20",
    "resend": "^6.4.2",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.9",
    "@types/nodemailer": "^6.4.5",
    "@types/pdfmake": "^0.2.12",
    "@types/uuid": "^9.0.7",
    "@types/xlsx": "^0.0.35",
    "nodemon": "^3.0.1",
    "tsx": "^4.1.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
